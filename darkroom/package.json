{
  "name": "darkroom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --report",
    "target": "vue-cli-service build --target app --name app",
    "help": "vue-cli-service build --help",
    "lint": "vue-cli-service lint",
    "inspect": "vue-cli-service inspect > outinput.js",
    "pub": "vue-cli-service inspect --plugins",
    "ui": "vue-cli-service ui"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "babel-preset-es2015": "^6.24.1",
    "captcha-mini": "^1.1.0",
    "compression-webpack-plugin": "^3.0.1",
    "core-js": "^3.3.2",
    "crypto-js": "^3.1.9-1",
    "cube-ui": "~1.12.15",
    "extract-text-webpack-plugin": "^3.0.2",
    "image-webpack-loader": "^6.0.0",
    "mint-ui": "^2.2.13",
    "moment": "^2.24.0",
    "node-rsa": "^1.0.7",
    "register-service-worker": "^1.6.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uws": "^100.0.1",
    "v-tap": "^3.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.3",
    "vue2-verify": "^1.1.5",
    "vuex": "^3.0.1",
    "webpack-spritesmith": "^1.0.2",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-cube-ui": "^0.2.5",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11",
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "transformModules": {
    "cube-ui": {
      "transform": "cube-ui/lib/${member}",
      "kebabCase": true,
      "style": {
        "ignore": [
          "create-api",
          "better-scroll",
          "locale"
        ]
      }
    }
  },
  "main": "index.js",
  "license": "MIT"
}