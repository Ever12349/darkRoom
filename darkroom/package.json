    "cube-ui": "~1.12.15",
    "mint-ui": "^2.2.13",
    "moment": "^2.24.0",
    "node-rsa": "^1.0.7",
    "register-service-worker": "^1.6.2",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue2-verify": "^1.1.5",
    "vuex": "^3.0.1",
    "xss": "^1.0.6"
